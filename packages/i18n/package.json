  "homepage": "https://vee-validate.logaretm.com/v4/guide/i18n",
  "license": "MIT",
  "module": "dist/vee-validate-i18n.mjs",
  "unpkg": "dist/vee-validate-i18n.iife.js",
  "main": "dist/vee-validate-i18n.mjs",
  "types": "dist/vee-validate-i18n.d.ts",
  "type": "module",
